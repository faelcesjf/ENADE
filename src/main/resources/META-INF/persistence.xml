<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ENADEPU" transaction-type="RESOURCE_LOCAL">
    <class>br.edu.uniacademia.enade.model.TbProva</class>
    <class>br.edu.uniacademia.enade.model.TbQuestao</class>
    <class>br.edu.uniacademia.enade.model.TbResultado</class>
    <class>br.edu.uniacademia.enade.model.TbTipoQuestao</class>
    <class>br.edu.uniacademia.enade.model.TbUsuario</class>
    <class>br.edu.uniacademia.enade.model.TbTipoUsuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/enade?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
